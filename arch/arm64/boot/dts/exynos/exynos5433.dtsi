			assigned-clock-parents = <&cmu_top CLK_ACLK_MSCL_400>,
						 <&cmu_top CLK_SCLK_JPEG_MSCL>,
						 <&cmu_mscl CLK_MOUT_SCLK_JPEG_USER>,
						 <&cmu_top CLK_MOUT_BUS_PLL_USER>;
			iommus = <&sysmmu_jpeg>;
		};

